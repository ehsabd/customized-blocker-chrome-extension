{
  "manifest_version": 2,
  "name": "Customized Blocker",
  "version": "0.1",
  "content_scripts": [
    {
      "js": ["jquery.min.js", "custom-block.js"],
      "matches":["http://*/*", "https://*/*"]
      
    }

  ],
  "browser_action": {
    "default_popup": "popup.html",
    
  },
  "permissions": ["storage"]
}
